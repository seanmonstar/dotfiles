# This tmux statusbar config was created by tmuxline.vim
# on Mon, 27 Apr 2015

set -g status-bg "colour254"
set -g message-command-fg "colour255"
set -g status-justify "left"
set -g status-left-length "100"
set -g status "on"
set -g pane-active-border-fg "colour33"
set -g message-bg "colour240"
set -g status-right-length "100"
set -g status-right-attr "none"
set -g message-fg "colour255"
set -g message-command-bg "colour240"
set -g status-attr "none"
set -g status-utf8 "on"
set -g pane-border-fg "colour240"
set -g status-left-attr "none"
setw -g window-status-fg "colour245"
setw -g window-status-attr "none"
setw -g window-status-activity-bg "colour254"
setw -g window-status-activity-attr "none"
setw -g window-status-activity-fg "colour33"
setw -g window-status-separator ""
setw -g window-status-bg "colour254"
set -g status-left "#[fg=colour255,bg=colour33] #S #[fg=colour33,bg=colour254,nobold,nounderscore,noitalics]"
set -g status-right "#[fg=colour240,bg=colour254,nobold,nounderscore,noitalics]#[fg=colour255,bg=colour240] %Y-%m-%d | %H:%M #[fg=colour33,bg=colour240,nobold,nounderscore,noitalics]#[fg=colour255,bg=colour33] #h "
setw -g window-status-format "#[fg=colour245,bg=colour254] #I |#[fg=colour245,bg=colour254] #W "
setw -g window-status-current-format "#[fg=colour254,bg=colour240,nobold,nounderscore,noitalics]#[fg=colour255,bg=colour240] #I |#[fg=colour255,bg=colour240] #W #[fg=colour240,bg=colour254,nobold,nounderscore,noitalics]"
